<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2+</version>
	</compatibility>
	<theme id="the-outdoors-life" name="The Outdoors Life">
		<root-path>/</root-path>
		<templates-path>${root-path}/templates</templates-path>
		<images-path>${root-path}/images</images-path>
		<template-extension>vm</template-extension>
		<settings>
			<setting key="portlet-setup-show-borders-default" value="false"></setting>
		</settings>
		<layout-templates>
			<custom>
				<layout-template id="1_2_1_columns_80_20" name="1-2-1 Columns (80/20)">
					<template-path>/layouttpl/custom/1_2_1_columns.tpl</template-path>
					<wap-template-path>/layouttpl/custom/1_2_1_columns.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/1_2_1_columns.png</thumbnail-path>
				</layout-template>
			</custom>
		</layout-templates>
	</theme>
</look-and-feel>